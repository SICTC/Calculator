program: Sample;
# This is a sample program in SICTC.
   Comments appear within pound signs.  #
var: a, x, s, t; # all variables are integers #
begin:
  a:=100;
  if: (a>0), then:
    x:=1; s:=0;
    while: (x<=a), do:
      s:=(s+x); x:=(x+1);
    end.
  end.
  t:=((a*(a+1))/2);
  print: s;
  print: t;
halt.